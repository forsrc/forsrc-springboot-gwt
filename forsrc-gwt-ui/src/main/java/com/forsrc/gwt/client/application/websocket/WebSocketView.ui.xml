<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
       xmlns:g="urn:import:com.google.gwt.user.client.ui"
       xmlns:m="urn:import:gwt.material.design.client.ui"
       xmlns:m.html="urn:import:gwt.material.design.client.ui.html"
       xmlns:ma="urn:import:gwt.material.design.addins.client">

    <g:HTMLPanel>

        <m:MaterialRow>
            <m:MaterialRow marginLeft="5" marginRight="5">
                <m:MaterialCard backgroundColor="BLUE" grid="l10 m12 s12" float="RIGHT">
                    <m:MaterialPanel backgroundColor="BLUE_DARKEN_2" overflow="HIDDEN" padding="12">
                        <m:MaterialIcon iconType="MORE_VERT" float="RIGHT" textColor="WHITE"/>
                        <m:MaterialIcon iconType="CLOSE" float="RIGHT" textColor="WHITE"/>
                    </m:MaterialPanel>
                    <m:MaterialCardContent>
                        <m:MaterialRow marginBottom="0">
                            <m:MaterialImage backgroundColor="BLUE_ACCENT_1" marginRight="12" marginTop="8" float="LEFT" width="40px" height="40px" shadow="1" circle="true" url="https://mir-s3-cdn-cf.behance.net/project_modules/disp/70e0a922433737.5631e83fc9429.png"/>
                            <ma:bubble.MaterialBubble backgroundColor="BLUE_ACCENT_1" position="LEFT" float="LEFT">
                                <m:MaterialLabel text="Hello there, How are you doing?"/>
                                <m:MaterialLabel text="1/2/2016" fontSize="0.6em" float="RIGHT"/>
                            </ma:bubble.MaterialBubble>
                        </m:MaterialRow>
                        <m:MaterialRow marginBottom="0">
                            <m:MaterialImage backgroundColor="BLUE_DARKEN_3" marginLeft="12" marginTop="8" float="RIGHT" width="40px" height="40px" shadow="1" circle="true" url="https://mir-s3-cdn-cf.behance.net/project_modules/disp/6ba40822433737.5632180017216.png"/>
                            <ma:bubble.MaterialBubble textColor="WHITE" backgroundColor="BLUE_DARKEN_3" position="RIGHT" float="RIGHT">
                                <m:MaterialLabel text="Hi there, Im fine, thank you. How about you?"/>
                                <m:MaterialLabel text="1/2/2016" fontSize="0.6em" float="RIGHT"/>
                            </ma:bubble.MaterialBubble>
                        </m:MaterialRow>
                        <m:MaterialRow marginBottom="0">
                            <m:MaterialImage backgroundColor="BLUE_DARKEN_3" marginLeft="12" marginTop="8" float="RIGHT" width="40px" height="40px" shadow="1" circle="true" url="https://mir-s3-cdn-cf.behance.net/project_modules/disp/6ba40822433737.5632180017216.png"/>
                            <ma:bubble.MaterialBubble textColor="WHITE" backgroundColor="BLUE_DARKEN_3" position="RIGHT" float="RIGHT">
                                <m:MaterialLabel text="Me too, Im doing good."/>
                                <m:MaterialLabel text="1/2/2016" fontSize="0.6em" float="RIGHT"/>
                            </ma:bubble.MaterialBubble>
                        </m:MaterialRow>
                        <m:MaterialRow marginBottom="0">
                            <m:MaterialImage backgroundColor="BLUE_ACCENT_1" marginRight="12" marginTop="8" float="LEFT" width="40px" height="40px" shadow="1" circle="true" url="https://mir-s3-cdn-cf.behance.net/project_modules/disp/70e0a922433737.5631e83fc9429.png"/>
                            <ma:bubble.MaterialBubble backgroundColor="BLUE_ACCENT_1" position="LEFT" float="LEFT">
                                <m:MaterialLabel text="Would you want to go to Philippines?"/>
                                <m:MaterialLabel text="1/2/2016" fontSize="0.6em" float="RIGHT"/>
                            </ma:bubble.MaterialBubble>
                        </m:MaterialRow>
                        <m:MaterialRow marginBottom="0">
                            <m:MaterialImage textColor="WHITE" backgroundColor="RED" marginRight="12" marginTop="8" float="LEFT" width="40px" height="40px" shadow="1" circle="true" url="https://mir-s3-cdn-cf.behance.net/project_modules/disp/1ccdd322433737.5631e853db4a9.png"/>
                            <ma:bubble.MaterialBubble textColor="WHITE" backgroundColor="RED" position="LEFT" float="LEFT">
                                <m:MaterialLabel text="Hello there, How are you doing?"/>
                                <m:MaterialLabel text="1/2/2016" fontSize="0.6em" float="RIGHT"/>
                            </ma:bubble.MaterialBubble>
                        </m:MaterialRow>
                        <m:MaterialRow marginBottom="0">
                            <m:MaterialImage backgroundColor="BLUE_DARKEN_3" marginLeft="12" marginTop="8" float="RIGHT" width="40px" height="40px" shadow="1" circle="true" url="https://mir-s3-cdn-cf.behance.net/project_modules/disp/6ba40822433737.5632180017216.png"/>
                            <ma:bubble.MaterialBubble textColor="WHITE" backgroundColor="BLUE_DARKEN_3" position="RIGHT" float="RIGHT">
                                <m:MaterialLabel text="Yes of course, I want to visit Philippines and take a tour."/>
                                <m:MaterialLabel text="1/2/2016" fontSize="0.6em" float="RIGHT"/>
                            </ma:bubble.MaterialBubble>
                        </m:MaterialRow>
                    </m:MaterialCardContent>
                </m:MaterialCard>
            </m:MaterialRow>
        </m:MaterialRow>
        <m:MaterialLabel ui:field="searchLabel"/>
    </g:HTMLPanel>
</ui:UiBinder>
